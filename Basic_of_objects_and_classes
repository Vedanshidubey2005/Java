class Student{
    int age;
    String name;
    float marks = 90; //if you gave here value and not initialzed in main class
}
public class basic3 {
    public static void main(String[] args) {
        Student S1 = new Student();
        // System.out.println(S1);      //Student@28a418fc , output when not initialized
        System.out.println(S1.age);
        System.out.println(S1.name);
        System.out.println(S1.marks);
        //output when variables are not initilaized , so they are giving default values
        //         0
        // null
        // 0.0
        S1.age = 18;
        S1.name= "Vedanshi Dubey";
        S1.marks = 91;
        System.out.println(S1.age);
        System.out.println(S1.name);
        System.out.println(S1.marks);
         // 18
        // Vedanshi Dubey
        // 91.0
    }
}
